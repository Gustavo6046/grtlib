<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Raytracer" />
		<Option makefile="makefile" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Raytracer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="sfml-graphics-d" />
					<Add library="sfml-window-d" />
					<Add library="sfml-system-d" />
					<Add directory="$(#sfml.lib)" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Raytracer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="sfml-graphics" />
					<Add library="sfml-window" />
					<Add library="sfml-system" />
					<Add directory="$(#sfml.lib)" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DSFML_DYNAMIC" />
			<Add directory="$(#sfml.include)" />
		</Compiler>
		<Linker>
			<Add library="opengl32" />
			<Add directory="$(#sfml.lib)" />
		</Linker>
		<Unit filename=".gitignore" />
		<Unit filename="CMakeLists.txt" />
		<Unit filename="Camera.hpp" />
		<Unit filename="Line.hpp" />
		<Unit filename="Raymath.cpp" />
		<Unit filename="Raymath.hpp" />
		<Unit filename="Raytracer.cpp" />
		<Unit filename="Raytracer.hpp" />
		<Unit filename="RenderObj.cpp" />
		<Unit filename="RenderObj.hpp" />
		<Unit filename="Renderer.cpp" />
		<Unit filename="Renderer.hpp" />
		<Unit filename="Scene.cpp" />
		<Unit filename="Scene.hpp" />
		<Unit filename="Sphere.cpp" />
		<Unit filename="Sphere.hpp" />
		<Unit filename="Triangle.cpp" />
		<Unit filename="Triangle.hpp" />
		<Unit filename="Vector.cpp" />
		<Unit filename="Vector.hpp" />
		<Unit filename="deps/CMakeLists.txt" />
		<Unit filename="main.cpp" />
		<Unit filename="raytri.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
